            android:label="@string/app_name"
            android:screenOrientation="portrait"
             >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BreedList"
            android:label="@string/title_activity_breed_list" >
        </activity>
        <activity
            android:name=".BreedDisplayActivity"
            android:label="@string/title_activity_breed_display"
