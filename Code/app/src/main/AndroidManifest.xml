        <activity
            android:name=".QuizQuestionsActivity"
            android:label="@string/title_activity_quiz_questions"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".QuizResultActivity"
            android:label="@string/title_activity_quiz_result" >
        </activity>
        <activity
            android:name=".BudgetMainActivity"
            android:label="@string/title_activity_budget_main"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".BudgetResultActivity"
            android:label="@string/title_activity_budget_result" >
        </activity>
        <activity
            android:name=".ClinicCityListViewActivity"
            android:label="@string/title_activity_clinic_city_list_view" >
        </activity>
        <activity
            android:name=".ClinicsDisplayActivity"
            android:label="@string/title_activity_clinics_display" >
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyD0R5vVOtZbHbTqZDdeqw4jvWetbkVV2Gg" />

        <activity
            android:name=".ClinicMapsActivity"
            android:label="@string/title_activity_clinic_maps"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".PicturesPageActivity"
            android:label="@string/title_activity_calendar_main">
        </activity>
        <activity
            android:name=".PictureSaveActivity"
            android:label="@string/title_activity_picture_save"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
